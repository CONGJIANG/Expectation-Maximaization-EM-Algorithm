#include <iostream>
using namespace std;

void p1()
{
 double height;
 cin >> height;
 double inch; //find inch height
 inch = height/2.54;
 //cout << "convert height is" << inch << "inches" << endl;
 int feet;
 feet = inch/12;
    cout << feet << "feets" << inch-feet*12 << "inches" << endl;
}


void p2()
{
 int year;
 cin >> year;
 if (year % 400 == 0)
  cout << year << "is leap year";
  else if (year % 100 == 0)
   cout << year << "is not leap year";
   else if (year % 4 ==0)
    cout << year << "is leap year";
 }

void p3()
{
int number;
cin >> number;
int k;
for (int i=1; i<= number; i++)
 for (int j=1; j<= number; j++)
 {
  k=i*j;
        cout << k << endl;
 }
}

int main() {
 p1();
 p2();
 p3();
 return 0;
}
